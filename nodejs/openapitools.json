{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "generators": {
      "swiftref": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/swiftref/api/generated",
        "glob": "openapi/SWIFT-API-swiftref_api-4.0.0-swagger.yaml"
      },
      "preval": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/preval/api/generated",
        "glob": "openapi/SWIFT-API-payment-validation-2.4.1-resolved-2.yaml"
      },
      "messaging-api": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/src/messaging-api/api/generated",
        "glob": "openapi/SWIFT-API-Swift-Messaging-1.1.0-resolved.yaml"
      }
    }
  }
}
