{
  "name": "api-sample-code",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "postinstall": "openapi-generator-cli generate",
    "swiftref": "cross-env DOTENV_CONFIG_PATH=.env.swiftref node -r dotenv/config -r ts-node/register src/swiftref/app.ts",
    "preval": "cross-env DOTENV_CONFIG_PATH=.env.preval node -r dotenv/config -r ts-node/register src/preval/app.ts",
    "messaging-api": "cross-env DOTENV_CONFIG_PATH=.env.messaging-api node -r dotenv/config -r ts-node/register src/messaging-api/app.ts",
    "test": "npm run test-swiftref && npm run test-preval && npm run test-messaging-api",
    "test-swiftref": "cross-env DOTENV_CONFIG_PATH=.env.swiftref jest --setupFiles=dotenv/config --testTimeout=5000 --detectOpenHandles swiftref.test.js",
    "test-preval": "cross-env DOTENV_CONFIG_PATH=.env.preval jest --setupFiles=dotenv/config --testTimeout=5000 --detectOpenHandles preval.test.js",
    "test-messaging-api": "cross-env DOTENV_CONFIG_PATH=.env.messaging-api jest --setupFiles=dotenv/config --testTimeout=5000 --detectOpenHandles messaging-api.test.js",
    "lint": "eslint --fix ."
  },
  "dependencies": {
    "axios": "1.6.7",
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "https-proxy-agent": "7.0.4",
    "node-forge": "1.3.1",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.13.4",
    "@types/jest": "29.5.12",
    "@types/node-forge": "1.3.11",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "supertest": "6.3.4",
    "ts-jest": "29.1.5",
    "ts-node": "10.9.2",
    "typescript": "^5"
  }
}
